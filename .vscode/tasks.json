{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "build -O0",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"-Wpedantic",
				"-Wextra",
				"-Wconversion",
				"-Wunused",
				"-Werror",
				"-std=c++20",
				"${fileDirname}/${fileBasenameNoExtension}.cpp",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.out",
				"-I${workspaceFolder}/Utils"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "cppbuild",
			"label": "build -O3",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"-Wpedantic",
				"-Wextra",
				"-Wconversion",
				"-Wunused",
				"-Werror",
				"-std=c++20",
				"${fileDirname}/${fileBasenameNoExtension}.cpp",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}.out",
				"-I${workspaceFolder}/Utils"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		}
	]
}